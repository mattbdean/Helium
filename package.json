{
  "name": "helium",
  "version": "1.1.2",
  "description": "Svoboda Lab experiment metadata collection and standardization",
  "scripts": {
    "benchmark": "ts-node server/benchmarks/schema-dao.bench.ts",
    "build": "yarn build:dev",
    "build:dev": "gulp build && ng build",
    "build:prod": "gulp build && ng build --prod",
    "build:preview": "ng build -c preview --output-path docs/preview --base-href \"/Helium/preview\" --deploy-url \"/Helium/preview/\"",
    "dev": "gulp build && concurrently --raw \"nodemon dist\" \"gulp watch\" \"ng build --watch\"",
    "e2e": "yarn build --progress=false && concurrently --raw --kill-others --success first \"node dist\" \"sleep 1 && yarn e2e:prepped\"",
    "e2e:debug": "yarn build && concurrently --raw --kill-others \"node dist\" \"sleep 1 && yarn e2e:prepped -- -ee\"",
    "e2e:prepped": "ng e2e",
    "preview": "ng serve -c preview --optimization false",
    "prod": "gulp build && ng build --prod && node dist",
    "test": "yarn test:server && yarn test:client && yarn e2e && ng build --prod",
    "test:server": "gulp testPrep && TS_NODE_PROJECT=server/tsconfig.json nyc mocha --opts server/test/mocha.opts",
    "test:client": "ng test --watch false --source-map false --code-coverage",
    "prepublish": "yarn snyk protect"
  },
  "author": "Matthew Dean <deanmatthew16@gmail.com>",
  "repository": "mattbdean/Helium",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
