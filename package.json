    "@angular/material": "~7.2.2",
    "@angular/platform-browser": "~7.2.2",
    "@angular/platform-browser-dynamic": "~7.2.2",
    "@angular/router": "~7.2.2",
    "body-parser": "^1.17.1",
    "chalk": "^2.1.0",
    "compression": "^1.7.2",
    "core-js": "^2.5.0",
    "express": "^4.15.4",
    "express-list-endpoints": "^3.0.0",
    "express-paginate": "^1.0.0",
    "faker": "^4.1.0",
